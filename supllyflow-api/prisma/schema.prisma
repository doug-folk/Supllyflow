











model Supplier {
  id       String    @id @default(uuid())
  name     String
  contact     String
  description String
   category    String
  user     User      @relation(fields: [userId], references: [id])
  userId   String
  products Product[]

  @@map("suppliers")
}
model Product {
  id         String   @id @default(uuid())
  name       String
  user       User     @relation(fields: [userId], references: [id])
  userId     String
   description String
   bar_code    String   @unique
  amount      Int
  category    String
  supplier   Supplier @relation(fields: [supplierId], references: [id])
  supplierId String

  @@map("products")
}